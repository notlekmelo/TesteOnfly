    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@notlekmelo/gerar-boletos": "^1.5.3",
    "@types/jsdom": "^20.0.0",
    "argon2": "^0.30.1",
    "bcrypt": "^5.1.0",
    "cache-manager": "^4.0.1",
    "cache-manager-redis-store": "^2.0.0",
    "chai": "4.3.0",
    "chai-http": "4.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "jsdom": "^20.0.0",
    "mime-types": "^2.1.35",
    "mssql": "^9.1.1",
    "nestjs-typeorm-paginate": "^4.0.2",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
