  "scripts": {
    "tsc": "tsc",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/src/main",
    "dev": "set NODE_ENV=development&&ts-node -r tsconfig-paths/register src/main",
    "debug": "nest start --debug --watch",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/infra/typeorm/data-source.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./src/infra/typeorm/data-source.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/infra/typeorm/data-source.ts migration:revert",
    "test:module": "jest -i $1 --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.2",
    "@nestjs/config": "^2.2.0",
